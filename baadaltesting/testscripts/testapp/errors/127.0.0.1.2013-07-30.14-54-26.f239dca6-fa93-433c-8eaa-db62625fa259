S'# coding: utf8\nimport os\nimport logging\nimport datetime\nimport logging.config\nfrom helper import xml_connect\nfrom helper import db_connection\nfrom helper import get_context_path\nfrom selenium import webdriver\nfrom selenium.webdriver.common.keys import Keys\nimport sys\n\n#creating a logger for logging the records\nlogging.config.fileConfig(os.path.join(get_context_path(),\'static/logging.conf\'))\nlogger = logging.getLogger("testing baadal")\n\n#creating connection to remote database\nbaadal_db=db_connection() \n\n#########################################################################################\n#                                The main test function                                 #\n#########################################################################################\n\t\t\ndef test_script(test_case_no):\n    root = xml_connect()\n    num=int(test_case_no)\n    if root[num-1].get("id")==test_case_no:\n        i=num-1\n        for j in xrange(0,len(root[i])):\n            driver = webdriver.Firefox()#connect to selenium server\n            driver.implicitly_wait(10)\n            driver.get(root[i].get("url")) #url of the page to be hit \n            driver.find_element_by_link_text(root[i].get("href")).click()\n            for k in xrange(0,len(root[i][j])):\n                field_type=root[i][j][k].get("type")\n                user_name=""\n                xml_parent=root[i]\n                xml_child=root[i][j]\n                xml_sub_child=root[i][j][k]\n\t\t\t\t\t\n                if field_type=="input": #checking for text fields\n                    user_name=isInput(driver,xml_sub_child)\n                 \n                elif field_type=="read_only": #checking for submit button\n                    isReadOnly(driver, xml_parent,xml_child,xml_sub_child,user_name)\n\t\t\t\t\t\t\n                elif field_type=="submit": #checking for submit button\n                    isSubmit(driver, xml_parent,xml_child,xml_sub_child,user_name)\n\t\t\t\t\t\t\n                elif field_type=="button":#checking for button\n                    isButton(driver,xml_sub_child)\n\t\t\t\t\t\n                elif field_type=="scroll":#scrolling the page up/down\n                    isScroll(driver,xml_sub_child)\n\t\t\t\t \t\t\n                elif field_type=="href":\n                    isHref(driver,xml_sub_child)#clicking on the hyper link\n                    \n                elif field_type=="select":\n                    isSelect(driver,xml_sub_child)# selecting from dropdown menu\n\t\t\t \t\n                elif field_type=="table":\n                    isTable(driver,xml_parent,xml_child,xml_sub_child)#checking for data in table\n                         \n                elif field_type=="img":#checking for setting image\n                    a=xml_subchild.text\n                    isImage(driver,xml_sub_child,a)\n\t\t\t\t\t\t\t\t\n                elif field_type=="tagname":#cheking for host table\n                    isCheckTable(driver,xml_parent,xml_child,xml_sub_child)\n\t\t\t\t\t\t\t\t\n                else:\n                    logging.debug("report problem") #logging the report\n            driver.close()#disconnect from server\n    baadal_db.commit()\n    #cursor3.close()\n    baadal_db.close()\n\n\t\t\n########################################################################################\n#                  Functions used by the input field                                      #\n########################################################################################\t\t\n\n# checking whether an element is present on the webpage\ndef isElementPresent(driver, xpath):\n\ttry:\n\t\tdriver.find_element_by_xpath(xpath)\n\t\treturn 1\n\texcept:\n\t\tprint "No Table exists"\n\t\treturn 0\t\n\t\t\n#checking whether front end data and daatabase entries are equal and printing the result \t\t\ndef print_result(field_text,result,xml_child):\n\tquery_result=str(result)\n\tif field_text==query_result:\n\t\tlogger.debug(xml_child.get("value") +\': Result:correct input\') \n\telse:\n\t\tlogger.debug(xml_child.get("value") +\': Result:Incorrect input\')\n\treturn \n\t\ndef open_error_page(driver,text):#open error link on differnet page\n\tdriver.find_element_by_link_text(text).click()\n\tfield=driver.find_element_by_xpath("//span[@id=\'overlayblock\']/div")\n\terror_message=field.text\n\tdriver.find_element_by_link_text("CLOSE WINDOW").click()\n\tfield=driver.find_element_by_tag_name("html")\n\tfield.send_keys("Keys.PAGE_DOWN")\n\tdriver.execute_script("window.scrollBy(0,200)", "")\n\treturn error_message\n\t\ndef host_status(status):#checking host status from database and giving them value\n\thost={0:"Down",1:"Up",2:"Maintenance"}\t\n\tif status==0:\n\t\tresult=host[0]\n\tif status==1:\n\t\tresult=host[1]\n\tif status==2:\n\t\tresult=host[2]\n\treturn result\n\t\ndef org_task_status(status):#checking org_task status from database and giving them value\n\ttask_status={0:"Approve  |  Reject",3:"Installation In Progress"}\n\tif\tstatus==0 | status==2 :\n\t\tresult=task_status[0]\n\tif status==3:\n\t\tresult=task_status[3]\n\treturn result\n\t\n#########################################################################################\n#  \t\t\t\t\tfunctions for various types of input fields  \t\t\t\t\t\t#\n#########################################################################################\n\t\t\ndef isInput(driver, xml_sub_child):\n\tcurrent_time=datetime.datetime.now().strftime("%Y-%m-%d_%H:%M:%S")\n\tfield = driver.find_element_by_id(xml_sub_child.get("id"))\n\tif xml_sub_child.text!=None:\n\t\tfield.send_keys(xml_sub_child.text) # sending the user name/password/vm name/purpose etc\n\t\tif xml_sub_child.get("id")=="user_username":\t\n\t\t\tuser_name=(xml_sub_child.text)\n\t\telse:\n\t\t\tuser_name=" "\n\telse:\n\t\n\t\tif xml_sub_child.get("id")=="user_username":\t\n\t\t\tuser_name=" "\n\t\telse:\n\t\t\tfield.send_keys(str(current_time))\n\t\t\tuser_name=" "\n\treturn user_name\n\t\ndef isReadOnly(driver, xml_parent,xml_child,xml_sub_child,user_name):\n    current_time=datetime.datetime.now().strftime("%Y-%m-%d_%H:%M:%S")\n    field = driver.find_elements_by_id(xml_sub_child.get("id"))\n    print xml_sub_child.get("id")+"="+field[0].text\n    #print \n    if field[0].text!=\'\':\n        field[0].send_keys(xml_sub_child.text) # sending the user name/password/vm name/purpose etc\n        if field[0].text==xml_sub_child.text:\n            print "error"\n        else:\n            print "no error"\n    else:\n        print "empty"\n    return \n\ndef isSubmit( driver, xml_parent, xml_child, xml_sub_child, user_name):\n    current_time=datetime.datetime.now()\n    driver.find_element_by_xpath(xml_sub_child.text).click()\n    cursor1=baadal_db.cursor()\n    cursor1.execute(xml_parent.get("query1"),str(user_name)) \n    row= cursor1.fetchone()\n    print user_name\n    if cursor1.rowcount!=0:\n        cursor2=baadal_db.cursor() #create object of database cursor\n        cursor2.execute(xml_parent.get("query2"),(str(row[0]))) #execute sql query\n        row = cursor2.fetchone()\n        if cursor2.rowcount!=0:\n            print row\n            if (row[0]>=current_time) | ("Logged-in" in str(row[1])):\n                print "1"\n                logger.debug(xml_child.get("value")  +\': Result:Valid Login\') #logging the report\n            else:\n                logger.debug(xml_child.get("value")  +\': Result:Invalid Login\') #logging the report\n                print "2"\n        else:\n            logger.debug(xml_child.get("value") +\': Result:Invalid Loginss\') #logging the report\n            print "3"\n        cursor2.close()\n    else:\n        logger.debug(xml_child.get("value") +\': Result:Invalid Loginss\') #logging the report\n        print "4"\n    cursor1.close()\n    return\n\n\n\t\ndef isButton(driver, xml_sub_child):\n\tdriver.find_element_by_xpath(xml_sub_child.text).click()\n\treturn\n\t\t\t\ndef isScroll(driver, xml_sub_child):\n\tfield=driver.find_element_by_tag_name("html")\n\tfield.send_keys(xml_sub_child.text)\n\tdriver.execute_script("window.scrollBy(0,200)", "")\n\treturn\n\t\ndef isHref(driver, xml_sub_child):\n\tdriver.find_element_by_partial_link_text(xml_sub_child.text).click()\n\treturn\n\ndef isSelect(driver, xml_sub_child):\n\tdriver.find_element_by_xpath(xml_sub_child.text).click()\n\treturn\n\t\ndef isImage(driver,xml_subchild,a):\t\n\tif isElementPresent(driver,a):\n\t\tif xml_subchild.get("name") in ["Pause","Gracefully","Forcefully","Migrate","Request","Lock","Edit","Setting","Resume"]:\n\t\t\tdriver.find_element_by_xpath(xml_subchild.text).click()\n\treturn\n\ndef isTable(driver,xml_parent,xml_child,xml_sub_child):\n\tlist={0:"Error",1:"failed_tasks",2:"TRY AGAIN | IGNORE",3:"list_my_vm",4:"Waiting for admin approval",5:"faculty_pending_tasks",6:"Add_host",7:"org_admin_pending_tasks",8:"org_admin_all_tasks"}\n\ttext=xml_sub_child.text\n\tif isElementPresent(driver,text):\n\t\tcursor3=baadal_db.cursor()\n\t\tcursor3.execute( xml_parent.get("query3"))\n\t\tquery3=cursor3.fetchall()\n\t\tlength=len(query3[0])#calculate number of columns of query\n\t\tr=0 #number of rows\n\t\tc=0 #number of columns\n\t\tfield=driver.find_elements_by_xpath(xml_sub_child.text)#data from gui\n\t\tfor col in field:\n\t\t\tfield_text=col.text\n\t\t\tif field_text!="":\n\t\t\t\tif field_text==list[0]:\n\t\t\t\t\ttext=open_error_page(driver,field_text)\n\t\t\t\t\tresult=query3[r][c]#data form query\n\t\t\t\t \tprint_result(field_text,result,xml_child)\n\t\t\t\t \t\n\t\t\t\telif (query3[r][c]==4) & (xml_parent.get("name")==list[1]):\n\t\t\t\t\tresult=list[2]\t\n\t\t\t\t\tprint_result(field_text,result,xml_child)\n\n\t\t\t\telif (query3[r][c]==2) & (xml_parent.get("name")==list[3]):\n\t\t\t\t\tresult=list[4]\n\t\t\t\t\tprint_result(field_text,result,xml_child)\n\t\t\t\t\t\n\t\t\t\telif (c%int(length)==6) & ((xml_parent.get("name")==list[5]) | (xml_parent.get("id")==list[7]) | (xml_parent.get("name")==list[8])):\n\t\t\t\t\tstatus=query3[r][c]\n\t\t\t\t\tresult=org_task_status(status)\n\t\t\t\t\tprint result\n\t\t\t\t\tprint_result(field_text,result,xml_child)\n\t\t\t\t\t\n\t\t\t\telif (c%int(length)==2) & (xml_parent.get("name")==list[6]):\n\t\t\t\t\tstatus=query3[r][c]\n\t\t\t\t\tresult=host_status(status)\n\t\t\t\t\tprint_result(field_text,result,xml_child)\n\t\t\t\t\t\t\n\t\t\t\telse:\n\t\t\t\t\tresult=query3[r][c]\n\t\t\t\t\tprint_result(field_text,result,xml_child)\n\t\t\t\tc=c+1\n\t\t\t\tif c%int(length)==0:\n\t\t\t\t\tr=r+1\n\t\t\t\t\tc=0\t\n\treturn\n\n\t\ndef isCheckTable(driver, xml_parent, xml_child, xml_sub_child):\n    field=driver.find_elements_by_class_name("title")\n    cursor4=baadal_db.cursor()\n    cursor4.execute(xml_parent.get("query3"))\n    query_result=cursor4.fetchall()\n    no_of_tables=cursor4.rowcount\n    for table in xrange(0,no_of_tables):\n        if query_result[table][0] in field[table].text:\n            text=xml_sub_child.text\n            if isElementPresent(driver,text):\n                cursor5=baadal_db.cursor()\t\n                cursor5.execute(xml_parent.get("query4"))\n                query3=cursor5.fetchall()\n                no_of_cols=len(query3[0])#calculate number of columns of query\n                field=driver.find_elements_by_xpath(xml_sub_child.text)\n                #print field.text\n                r=0\n                c=0\n                for col in field:\n                    field_text=col.text\n                    if field_text!="":\n                        result=query3[r][c]\n                        print_result(field_text,result,xml_child)\n                        c=c+1\n                        if c%int(no_of_cols)==0:\n                            r=r+1\n                            c=0\t\t\n                cursor5.close()\n    cursor4.close()\n    return\n'
