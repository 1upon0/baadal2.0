S'# coding: utf8\nimport logging\nimport datetime\nimport logging.config\nimport xml_connect\nfrom selenium import webdriver\nfrom selenium.webdriver.common.keys import Keys\nimport sys\n\n\ndef isElementPresent(driver, xpath):\n\ttry:\n\t\tdriver.find_element_by_xpath(xpath)\n\t\treturn 1\n\texcept:\n\t\tprint "No Table exists"\n\t\treturn 0\n\t\t\n\t\t\ndef ts(b):\n    db=db_connect.db_connection()\n    logging.config.fileConfig("static/logging.conf")\n    root = xml_connect.xml_connection()\n    num=int(b[0])\n    if root[num-1].get("id")==str(b[0]):\n        i=num-1\n        for j in xrange(0,len(root[i])):\n            logger = logging.getLogger(root[i].get("name"))# create logger\n            driver = webdriver.Firefox()#connect to server\n            driver.implicitly_wait(10)\n            driver.get(root[i].get("url")) #url to the page to be hit \n            driver.find_element_by_link_text(root[i].get("href")).click()\n            for k in xrange(0,len(root[i][j])):\n                field_type=root[i][j][k].get("type")\n                user_name=""\n                xml_parent=root[i]\n                xml_child=root[i][j]\n                xml_subchild=root[i][j][k]\n\t\t\t\t\t\n                if field_type=="input": #checking for input fields\n                    user_name=isInput(driver,xml_subchild)\n\t\t\t\t\t\t\n                elif field_type=="submit": #checking for submit button\n                    isSubmit(driver, xml_parent,xml_child,xml_subchild,user_name)\n\t\t\t\t\t\t\n                elif field_type=="button":#checking for submit button\n                    isButton(driver,xml_subchild)\n                        \n                elif field_type=="name":#checking for input fields\n                    isName(driver,xml_subchild)\n\t\t\t\t\t\n                elif field_type=="key":#scrolling the page down\n                    isKey(driver,xml_subchild)\n\t\t\t\t \t\t\n                elif field_type=="href":\n                    isHref(driver,xml_subchild)#redirecting on other page\n\t\t\t\t\t\t\n                elif field_type=="partialink":\n                    ispartialink(driver, xml_subchild)#redirecting on other page\n\t\t\t\t\t\t\n                elif field_type=="select":\n                    isSelect(driver,xml_subchild)# selecting from dropdown menu\n\t\t\t \t\n                elif field_type=="table":\n                    isTable(logger,driver,xml_parent,xml_child,xml_subchild)#checking for data in table\n                         \n                elif field_type=="img":#checking for setting image\n                    a=xml_subchild.text\n                    isImage(driver,xml_subchild,a)\n\t\t\t\t\t\t\t\t\n                elif field_type=="tagname":#cheking for host table\n                    isTagname(driver,xml_parent,xml_child,xml_subchild)\n\t\t\t\t\t\t\t\t\n                else:\n                    logging.debug("report problem") #logging the report\n            driver.close()#disconnect from server\n    db.commit()\n    #cursor3.close()\n    db.close()\n\n\t\t\n########################################################################################\n#                  Functions used in input fields                                      #\n########################################################################################\t\t\n\t\n\t\t\n\t\t\ndef print_result(logger,field_text,result,xml_child):#checking whether front end data and daatabase entries are equal and printing the result \n\tquery_result=str(result)\n\tif field_text==query_result:\n\t\tlogger.debug(xml_child.get("value") +\': Result:correct input\') \n\telse:\n\t\tlogger.debug(xml_child.get("value") +\': Result:Incorrect input\')\n\tprint query_result\n\tprint field_text\n\treturn \n\t\ndef open_error_page(driver,t):#open error link on differnet page\n\tdriver.find_element_by_link_text(t).click()\n\tb=driver.find_element_by_xpath("//span[@id=\'overlayblock\']/div")\n\tt=b.text\n\tdriver.find_element_by_link_text("CLOSE WINDOW").click()\n\tfield=driver.find_element_by_tag_name("html")\n\tfield.send_keys("Keys.PAGE_DOWN")\n\tdriver.execute_script("window.scrollBy(0,200)", "")\n\treturn t\n\t\ndef host_status(query):#checking host status from database and giving them value\n\thost={0:"Down",1:"Up",2:"Maintenance"}\t\n\tif query==0:\n\t\tresult=host[0]\n\tif query==1:\n\t\tresult=host[1]\n\tif query==2:\n\t\tresult=host[2]\n\treturn result\n\t\ndef org_task_status(query):#checking org_task status from database and giving them value\n\ttask_status={0:"Approve  |  Reject",3:"Installation In Progress"}\n\tif\tquery==0 | query==2 :\n\t\tresult=task_status[0]\n\tif query==3:\n\t\tresult=task_status[3]\n\treturn result\n\t\n#########################################################################################\n#  \t\t\t\t\tfunctions for various types of input fields  \t\t\t\t\t\t#\n#########################################################################################\n\t\t\ndef isInput(driver, xml_sub_child):\n\tcurrent_time=datetime.datetime.now().strftime("%Y-%m-%d_%H:%M:%S")\n\tfield = driver.find_element_by_id(xml_sub_child.get("id"))\n\tif xml_sub_child.text!=None:\n\t\tfield.send_keys(xml_sub_child.text) # sending the user name/password/vm name/purpose etc\n\t\tif xml_sub_child.get("id")=="user_username":\t\n\t\t\tuser_name=(xml_sub_child.text)\n\t\telse:\n\t\t\tuser_name=" "\n\telse:\n\t\n\t\tif xml_sub_child.get("id")=="user_username":\t\n\t\t\tuser_name=" "\n\t\tif xml_sub_child.get("id")=="vm_data_vm_name":\n\t\t\tfield.send_keys(str(current_time))\n\t\t\tuser_name=" "\n\treturn user_name\n\n\n\n\t\ndef isName(driver,xml_sub_child):\n\tcurrent_time=datetime.datetime.now().strftime("%Y-%m-%d_%H:%M:%S")\n\tfield=driver.find_element_by_name(xml_sub_child.get("id"))\n\tif xml_sub_child.text!=None:\n\t\tfield.send_keys(xml_sub_child.text)\n\telse:\n\t\tfield.send_keys(str(current_time))\n\treturn\t\n\t\n\n\ndef isTable(logger,driver,xml_parent,xml_child,xml_sub_child):\n\tlist={0:"Error",1:"failed_tasks",2:"TRY AGAIN | IGNORE",3:"list_my_vm",4:"Waiting for admin approval",5:"faculty_pending_tasks",6:"Add_host",7:"org_admin_pending_tasks",8:"org_admin_all_tasks"}\n\ta=xml_sub_child.text\n\tif isElementPresent(driver,a):\n\t\tcursor3=db.cursor()\n\t\tcursor3.execute( xml_parent.get("query3"))\n\t\tquery3=cursor3.fetchall()\n\t\tlength=len(query3[0])#calculate number of columns of query\n\t\tm=0\n\t\tp=0\n\t\tfield=driver.find_elements_by_xpath(xml_sub_child.text)#data from gui\n\t\tfor n in field:\n\t\t\tfield_text=n.text\n\t\t\tif field_text!="":\n\t\t\t\tif field_text==list[0]:\n\t\t\t\t\ttext=open_error_page(driver,t)\n\t\t\t\t\tresult=query3[m][p]#data form query\n\t\t\t\t \tprint_result(logger,field_text,result,xml_child)\n\t\t\t\t \t\n\t\t\t\telif (query3[m][p]==4) & (xml_parent.get("name")==list[1]):\n\t\t\t\t\tresult=list[2]\t\n\t\t\t\t\tprint_result(logger,field_text,result,xml_child)\n\n\t\t\t\telif (query3[m][p]==2) & (xml_parent.get("name")==list[3]):\n\t\t\t\t\tresult=list[4]\n\t\t\t\t\tprint_result(logger,field_text,result,xml_child)\n\t\t\t\t\t\n\t\t\t\telif (p%int(length)==6) & ((xml_parent.get("name")==list[5]) | (xml_parent.get("id")==list[7]) | (xml_parent.get("name")==list[8])):\n\t\t\t\t\tquery=query3[m][p]\n\t\t\t\t\tresult=org_task_status(query)\n\t\t\t\t\tprint result\n\t\t\t\t\tprint_result(logger,field_text,result,xml_child)\n\t\t\t\t\t\n\t\t\t\telif (p%int(length)==2) & (xml_parent.get("name")==list[6]):\n\t\t\t\t\tquery=query3[m][p]\n\t\t\t\t\tresult=host_status(query)\n\t\t\t\t\tprint_result(logger,field_text,result,xml_child)\n\t\t\t\t\t\t\n\t\t\t\telse:\n\t\t\t\t\tresult=query3[m][p]\n\t\t\t\t\tprint_result(logger,field_text,result,xml_child)\n\t\t\t\tp=p+1\n\t\t\t\tif p%int(length)==0:\n\t\t\t\t\tm=m+1\n\t\t\t\t\tp=0\t\n\treturn\n\ndef isSubmit( driver, xml_parent, xml_child, xml_sub_child, user_name):\n\tcurrent_time=datetime.datetime.now()\n\tdriver.find_element_by_xpath(xml_sub_child.text).click()\n\tcursor1=db.